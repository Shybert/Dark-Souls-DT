{
  "name": "dsdt",
  "version": "0.1.0",
  "description": "A death counter and timer application & overlay built for Dark Souls.",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron-webpack dev",
    "test": "jest"
  },
  "author": "Shybert",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shybert/Dark-Souls-DT"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/electron-store": "^1.3.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@vue/test-utils": "^1.0.0-beta.25",
    "electron": "^3.0.0",
    "electron-webpack": "^2.3.1",
    "electron-webpack-ts": "^2.1.1",
    "electron-webpack-vue": "^2.2.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3",
    "vue-class-component": "^6.2.0",
    "vue-jest": "^2.6.0",
    "vue-property-decorator": "^7.1.1",
    "vuex-type-helper": "^1.2.0",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "electron-store": "^2.0.0",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "jest": {
    "globals": {
      "vue-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "vue"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,vue}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text-summary"
    ]
  }
}
